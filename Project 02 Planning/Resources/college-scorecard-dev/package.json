    "sinon": "^1.17.2",
    "spec-xunit-file": "0.0.1-3",
    "stream-array": "^1.1.1",
    "tito": "^0.4.0",
    "uglify-js": "^3.4.9",
    "watchify": "^3.11.1",
    "wdio-mocha-framework": "^0.6.4",
    "webdriverio": "^4.14.4",
    "xmlhttprequest": "^1.8.0"
